{
  "name": "@pixelaw/vanilla",
  "private": false,
  "version": "0.7.11",
  "type": "module",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "author": "thiscaspar",
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "use-workspace": "node scripts/switchDependencies.js --workspace && pnpm install && pnpm build",
    "use-regular": "node scripts/switchDependencies.js && pnpm install && pnpm upgrade_pixelaw && pnpm build",
    "experiments": "tsx  src/scripts/experiments.ts",
    "build-serve": "vite build && http-server dist/",
    "dev": "NODE_ENV=development && vite",
    "build-prod": "tsc && vite build",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "coverage": "vitest run --coverage",
    "upgrade_pixelaw": "pnpm update @pixelaw/core @pixelaw/core-dojo @pixelaw/core-mud @pixelaw/react @pixelaw/react-dojo",
    "check": "biome check --apply ."
  },
  "dependencies": {
    "@pixelaw/core": "^0.8.2",
    "@pixelaw/core-dojo": "^0.8.2",
    "@pixelaw/core-mud": "^0.8.2",
    "@pixelaw/react": "^0.8.2",
    "@pixelaw/react-dojo": "^0.8.2",
    "@uiw/react-color-sketch": "^2.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.1",
    "unstorage": "^1.16.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@types/node": "^22.17.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "http-server": "^14.1.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-envs": "^4.6.2",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4"
  }
}
